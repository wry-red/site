---
// Short description of the item. 

export interface Props {
    desc: string;
}

const { desc } = Astro.props;
---

<p class="description">
    ${desc}
</p>

<style>
    .description {
        font-family:cursive;
        font-size:24px;
        color:#333;
        border: 30px;
        padding: auto;
    }
</style>